// Fake URIs in use
// units, site, filename, measurement, seasonality, boolean pos/neg, 
// precision, measurementData, measTable

{
    "@context" : {
        "xsd": "http://www.w3.org/2001/XMLSchema#",
        "schema" : "http://schema.org/",

        "paleoArchiveName" : "schema:name",
        "siteName" : "schema:site",
        "collectionName" : "schema:collection",
        "region" : "schema:containedIn",
        "country" : "schema:Country",
        "province": "schema:Province",
        "city": "schema:City",
        "geo" : "schema:geo",

        "latitude" : {

        	"@id"   : "schema:latitude",
            "@type" : "xsd:float",
            "units" : "schema:units",

        	"latMax": {
        		"@id" : "schema:maxValue"
        	},

        	"latMin": {
        		"@id" : "schema:minValue"
        	}
        },

        "longitude" : {

        	"@id"   : "schema:latitude",
            "@type" : "xsd:float",
            "units" : "schema:units",

        	"lonMax": {
        		"@id" : "schema:maxValue"
        	},

        	"lonMin": {
        		"@id" : "schema:minValue"
        	}
        },
        
        "elevation": {
            "@id": "schema:elevation",
            "@type": "xsd:float",
            "units" : "schema:units"
        },
        
        "archiveType" : "schema:alternateName",
        "investigator": "schema:name",
        "dateCreated": "schema:Date",
        "startDate": "schema:Date",
        "endDate" : "schema:Date",
        "whoCollected" : "schema:name",
        "whoAnalyzed" : "schema:name",
        "whoEnteredinDB" : "schema:name",
        "pubDOI": {
            "@id":"http://purl.org/ontology/bibo/doi",
            "@container": "@set"
        },
        "pubString" : "schema:alternateName",
        "dataDOI": {
            "@id":"http://purl.org/ontology/bibo/doi",
            "@container": "@set"
        },
 
        "columns" : {
            "@id" : "schema:description",
            "@container" : "@index"
        },
 
        "measurements" : {
            "@id" : "schema:measurementData",
            "@container" : "@index"
        },
        "measTable" : "schema:measurementTable",
        "measTableNumber" : {
            "@id" : "schema:name",
            "@value" : "xsd:integer"
        },
        "filename" : "schema:fileFormat",
        "chronologies" : {
            @id : "schema:description",
            "@container" : "@index"
        },
        "measRes" : {
            "@id" : "schema:measurement",
            "@type" : "xsd:float"
        },
        "longName" : "schema:description",
        "shortName" : "schema:alternateName",
        "proxy" : "schema:alternateName",
        "unit" : "schema:units",
        "parameter" : "schema:name",
        "parameterDetail" : "schema:description",
        "seasonality" : "schema:seasonality",
        "seasonality:value": {
            "@id" : "schema:value",
            "@type": "xsd:integer",
            "@container" : "@set"
        },
        "seasonality:string" : {
            "@id" : "shema:units",
            "@container" : "@set"
        },
        "interpDirection" : {
            "@id" : "schema:description",
        },
        "material" : "schema:description",
        "precision": {
            "@id": "schema:precision",
            "@type": "xsd:float"
        },
        "instrument" : "schema:device",
        "sensorModel" : "schema:description",
        "archiveModel" : "schema:description",
        "observationModel" : "schema:description",
 
        "chronTableName" : {
            "@id" : "schema:name",
            "@value" : "xsd:integer"
        },
        "chronName" : "schema:name",
        "chronType" : "schema:description",
        "chronRes" : "schema:description",
        "countRate": {
            "@container" : "@list"
        }
        "underCount" : {
            "@id" : "schema:maxValue",
            "@type" : "xsd:integer"
        },
        "overCount" : {
            "@id" : "schema:minValue",
            "@type" : "xsd:integer"
        },
        "errorLevel" : "schema:description"
    }
}
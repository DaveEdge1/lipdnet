div(ng-repeat='entry in files.json.chronData track by $index' layout-wrap="" ng-init="parentIdx = $index")

    // chronData[1] : Should only be one entry2. Don't need to loop
    div(ng-repeat="entry2 in entry.measurementTable track by $index" layout-wrap="")
        md-card(ng-show="entry2.tmp.toggle")
            md-subheader.md-primary chron {{parentIdx+1}} > measurement {{$index+1}}
            include ./table_root.jade
            // Toggle the csv entry2 box and the parse csv button. Once we parse CSV, these elements are not longer needed.
            include ./parse_csv.jade
                button.green-btn-off(ng-click="parseCsv(entry2, parentIdx, $index, {'pc': 'chron', 'tt': 'measurement'}); toggleCsvBox(entry2)" style="margin: 1.5em; float:right;" type="button") Parse Values
            br
            // Column Data
            include ./table_column.jade
            .md-toolbar-tools(style="background-color:white;")
                span(flex)
                button.red-btn-off(type="button" ng-click="removeBlock(entry.measurementTable, $index)" style="margin: 1.5em; padding: 2px 2px 2px 2px;") Delete Table

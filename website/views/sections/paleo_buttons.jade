div(ng-repeat='entry in files.json.paleoData track by $index' layout-wrap="" ng-init="parentIdx = $index")
    // paleoData[1] : Should only be one entry2. Don't need to loop


    //md-subheader.plain(ng-if='entry.measurementTable[0]') measurements
    //    md-tooltip {{getTooltip("paleoData", "measurement")}}

    div.btn-row(layout="row")
        div(ng-repeat='entry2 in entry.measurementTable track by $index')
            button.blue-btn-on(ng-if="entry2.tmp.toggle" ng-click="entry2.tmp.toggle = false" type="button") paleo {{parentIdx+1}} measurement {{$index+1}}
            button.blue-btn-off(ng-if="!entry2.tmp.toggle" ng-click="expandEntry(files.json.paleoData, entry2)" type="button") paleo {{parentIdx+1}} measurement {{$index+1}}

    //md-subheader.plain(ng-if='entry.model[0]') models
    //    md-tooltip {{getTooltip("paleoData", "model")}}

    div.btn-row(layout="row")
        div(ng-if='entry.model[0].method')
            button.blue-btn-on(ng-if="entry.model[0].method.tmp.toggle" ng-click="entry.model[0].method.tmp.toggle = false" type="button") paleo {{parentIdx+1}} model 1 method
            button.blue-btn-off(ng-if="!entry.model[0].method.tmp.toggle" ng-click="expandEntry(files.json.paleoData, entry.model[0].method)" type="button") paleo {{parentIdx+1}} model 1 method

        div(ng-repeat='entry2 in entry.model[0].summaryTable track by $index')
            button.blue-btn-on(ng-if="entry2.tmp.toggle" ng-click="entry2.tmp.toggle = false" type="button") paleo {{parentIdx+1}} model 1 summary {{$index+1}}
            button.blue-btn-off(ng-if="!entry2.tmp.toggle" ng-click="expandEntry(files.json.paleoData, entry2);" type="button") paleo {{parentIdx+1}} model 1 summary {{$index+1}}

        div(ng-repeat='entry2 in entry.model[0].ensembleTable track by $index')
            button.blue-btn-on(ng-if="entry2.tmp.toggle" ng-click="entry2.tmp.toggle = false" type="button") paleo {{parentIdx+1}} model 1 ensemble {{$index+1}}
            button.blue-btn-off(ng-if="!entry2.tmp.toggle" ng-click="expandEntry(files.json.paleoData, entry2)" type="button") paleo {{parentIdx+1}} model 1 ensemble {{$index+1}}


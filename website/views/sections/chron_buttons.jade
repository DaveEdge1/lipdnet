    div.btn-row(layout="row")

        div(ng-if='entry.model[0].method')
            button.blue-btn-on(ng-if="entry.model[0].method.tmp.toggle" ng-click="entry.model[0].method.tmp.toggle = false" type="button") chron {{parentIdx}} model 0 method
            button.blue-btn-off(ng-if="!entry.model[0].method.tmp.toggle" ng-click="expandEntry(files.json.chronData, entry.model[0].method)" type="button") chron {{parentIdx}} model 0 method

        div(ng-repeat='entry2 in entry.model[0].summaryTable track by $index')
            button.blue-btn-on(ng-if="entry2.tmp.toggle" ng-click="entry2.tmp.toggle = false" type="button") chron {{parentIdx}} model 0 summary {{$index}}
            button.blue-btn-off(ng-if="!entry2.tmp.toggle" ng-click="expandEntry(files.json.chronData, entry2)" type="button") chron {{parentIdx}} model 0 summary {{$index}}

        div(ng-repeat='entry2 in entry.model[0].ensembleTable track by $index')

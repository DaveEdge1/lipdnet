    div.btn-row(layout="row")

        div(ng-repeat='entry2 in entry.measurementTable track by $index')
            button.blue-btn-on(ng-if="entry2.tmp.toggle" ng-click="entry2.tmp.toggle = false" type="button") chron {{parentIdx}} measurement {{$index}}
            button.blue-btn-off(ng-if="!entry2.tmp.toggle" ng-click="expandEntry(files.json.chronData, entry2)" type="button") chron {{parentIdx}} measurement {{$index}}


    //md-subheader.plain(ng-if='entry.model[0]') models

md-toolbar.md-hue-1
	h2.md-toolbar-tools
		span PaleoData *

include ./paleo_meas.jade
include ./paleo_model.jade


div(layout='', layout-padding='', layout-sm='column' style="float: right;")
	md-input-container
		md-label Table Type
		md-select(ng-model='dropdowns.current.table' ng-model-options="{trackBy: '$value.id'}")

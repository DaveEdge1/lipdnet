.modal-content
    .modal-header
        h3.modal-title Interpretation {{idxNum + 1}}
    .modal-body(style="height:600px!important; overflow-y: auto;")
        div
            .col-md-4
                fieldset
                    .md-padding
                        div(layout='row', layout-wrap='')
                            div(flex='25', flex-gt-sm='50', layout='column')
                                h4 REQUIRED
                                div(layout='row', layout-wrap='' ng-repeat='key in required')
                                    md-checkbox(ng-checked='true', ng-click='toggle(key)' disabled) {{key}}
                                h4 STANDARD
                                div(layout='row', layout-wrap='' ng-repeat="key in standard")
                                    md-checkbox(ng-checked='exists(key)', ng-click='toggle(key)') {{key}}

                                h4 OPTIONAL
                                div(layout='row', layout-wrap='' ng-repeat='key in optional')
                                    md-checkbox(ng-checked='exists(key)', ng-click='toggle(key)') {{key}}
            .col-md-8
                div(ng-repeat="(k,v) in  currInterp" layout='', layout-padding='', layout-sm='column')
                    md-input-container(flex='', class="md-input-has-value")
                        label {{k}}
                        input(ng-model="currInterp[k]")

    .modal-footer
        button(ng-if="!new" style="float:left;" class="btn btn-danger" data-dismiss="modal" ng-click="delete()") DELETE
        button(class="btn btn-success" data-dismiss="modal" ng-click="close()") Save
        button(class="btn btn-danger" data-dismiss="modal" ng-click="dismiss()") Cancel

class PaleoArchive
{
  name;
  siteName;
  collectionName;
  region;
  float latMax;
  float latMin;
  latUnits;
  lonUnits;
  float lonMax;
  float lonMin;
  string pubDOI;
  pubString;
  dataDOI;
  float elevation;
  elevationUnits;
  archiveType;
  investigator;
  Integer year;
    1 -- 1 chronologies;
}

class HT01
{
  tableName;
filename;
chronType;
comments;
* -- 1 chronologies;
}

class measurements
{
  1 -- 1 PaleoArchive;
  1 -- 1 OM;
}
class OM
{
  tableName;
  filename;
  chronology;
   comments;
  }

class m1_col1
{
  longName;
  shortName;
  unit;
  1 -- 1 OM;
}

class m1_col2
{
  longName;
  shortName;
  unit;
  Integer yearMin;
  Integer yearMax;
  Integer measRes;
    1 -- 1 OM;
}

class m1_col3
{
  longName;
  shortName;
unit;
proxy;
1 -- 1 OM;
}

class m1_climateInterpretation
{
  parameter;
parameterDetail;
seasonality;
interpDirection;
1 -- 1 m1_col3;
}

class c1_col1
{
    longName;
shortName;
unit;
type;
1 -- 1 HT01;
}

class c1_col2
{
    longName;
shortName;
unit;
type;
1 -- 1 HT01;
}

class BSi
{
  tableName;
  filename;
  chronology;
    1 -- 1 measurements;
  1 -- 1 m2_col1;
  1 -- 1 m2_col2;
  1 -- 1 m2_col3;
}

class m2_col1
{
  longName;
  shortName;
  unit;
}

class m2_col2
{
  longName;
  shortName;
  unit;
  Integer yearMin;
  Integer yearMax;
  Integer measRes;
}

class m2_col3
{
  longName;
  shortName;
  unit;
  proxy;
}

class m2_climateInterpretation
{
  parameter;
  parameterDetail;
  seasonality;
  interpDirection;
  1 -- 1 m2_col3;
}

class chronologies
{
}

class c1_col3
{
      longName;
shortName;
unit;
comments;
1 -- 1 HT01;
}

class c1_col4
{
      longName;
shortName;
unit;
1 -- 1 HT01;
}

class c1_col5
{
      longName;
shortName;
unit;
type;
  comments;
1 -- 1 HT01;
}

class c1_col6
{
      longName;
shortName;
unit;
1 -- 1 HT01;
}

class c1_col7
{
      longName;
shortName;
unit;
errorLevel;
1 -- 1 HT01;
}

class c1_col8
{
  longName;
  shortName;
  unit;
  1 -- 1 HT01;
}

class c1_col9
{
  longName;
  shortName;
  unit;
  errorLevel;
  1 -- 1 HT01;
}


